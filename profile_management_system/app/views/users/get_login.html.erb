<% flash.each do |key, value| %>
	<div class="<%=flash_class(key)%>">
		<%= value %>
	</div>
<% end %>


<h1>Login</h1>
<br>
   <%= form_tag '/users/login' do %>
   <%= label_tag :Email_or_Phone%>
   <%= text_field_tag :email_or_phone_no %>
   <%= label_tag :Password%>
   <%= password_field_tag :password%>
   <%= submit_tag "Login"%>
<%end%>
<br>
<%= button_to 'Back', users_index_path, method: :get %>